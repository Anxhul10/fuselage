{
  "name": "fuselage-tamagui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "web": "yarn dev",
    "dev:vite": "NODE_ENV=development vite --host --clearScreen false",
    "dev:vite:extract": "EXTRACT=1 yarn dev",
    "dev": "yarn dev:webpack",
    "dev:webpack": "DISABLE_EXTRACTION=true NODE_ENV=development webpack serve",
    "dev:extract": "DISABLE_EXTRACTION=false NODE_ENV=development webpack serve",
    "prod": "NODE_ENV=production webpack --json=dist/compilation-stats.json",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-react-native-web": "^0.0.29",
    "@tamagui/config": "^1.125.32",
    "@tamagui/core": "~1.125.33",
    "@tamagui/lucide-icons": "~1.125.33",
    "@tamagui/vite-plugin": "~1.125.33",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tamagui": "~1.125.33",
    "tamagui-phosphor-icons": "~0.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@eslint/js": "^9.21.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/builder-vite": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^15.15.0",
    "playwright": "^1.51.1",
    "storybook": "^8.6.12",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.1.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
